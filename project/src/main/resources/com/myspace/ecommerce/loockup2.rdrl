package com.myspace.ecommerce;

import java.lang.Number;

rule "loockup2"
	dialect "mvel"
	salience 700
	ruleflow-group "gogo"
	no-loop true
	when
		ShipfromCity( sfCity == "Riyadh" )
		ShipToCity( stCity == "Jeddah" )
		v : VendorWorkingDays( kgfactor == null )
	then
		System.out.println("in loockup2");
		modify( v ) {
				setKgfactor( 0.8 ), 
				setFixedcost( 80.0 ), 
				setKmfactor( 0.0 ), 
				setLeadtime( 2.0 )
		}
end
